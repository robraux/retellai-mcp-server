    "@modelcontextprotocol/sdk": "^1.17.0",
    "dotenv": "^16.5.0",
    "retell-sdk": "4.41.0"
  },
  "bin": {
    "@robraux/retellai-mcp-server": "build/index.js"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
